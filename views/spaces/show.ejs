<header>
  <nav class="navbar navbar-expand-lg navbar-light bg-light">
    <a class="navbar-brand" href="/">ColorSpace</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/spaces">Back to Profile</a>
        <a class="nav-item nav-link" href="/auth/logout">Logout</a>
      </div>
    </div>
  </nav>
</header>

<h2>Individual Room Display Page</h2>

<hr />


<% space.colors.forEach(function(color) { %>
<div class="container">
  <div class="color-swatch-container <%= color.hex %>">
    <div class="color-inner">
      <div class="color-front">
        <footer class="details">
      <p>COLOR  <span><%= color.hex %></span></p>
        </footer>
      </div>
      <div class="color-back">
        <p>COLOR <%= color.hex %></p>
        <ul>
          <li>RGB <span><%= color.r %>,<%= color.g %>,<%= color.b %></span></li>
          <li>CMYK <span><%= color.c %>,<%= color.m %>,<%= color.y %>,<%= color.k %></span></li>
          <li>HEX <span><%= color.hex %></span></li>
        </ul>
    </div>
  </div>
</div>
<% }); %>


<hr />

<img src="<%= space.url %>" height="100" width="155">
